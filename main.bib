@article{rahbaran_microtca-based_2011,
	title = {{MicroTCA}-based {Global} {Trigger} {Upgrade} project for the {CMS} experiment at {LHC}},
	volume = {6},
	doi = {10.1088/1748-0221/6/12/C12054},
	abstract = {The electronics of the first Level Global Trigger (GT) of CMS is the last stage of the Level-1 trigger system [1]. At LHC up to 40 million collisions of proton bunches occur every second, resulting in about 800 million proton collisions. The CMS Level-1 Global Trigger [1], a custom designed electronics system based on FPGA technology and the VMEbus system, performs a quick on-line analysis of each collision every 25 ns and decides whether to reject or to accept it for further analysis. The CMS trigger group of the Institute of High Energy Physics in Vienna (HEPHY) is involved in the Level-1 trigger of the CMS experiment at CERN. As part of the Trigger Upgrade, the Level-1 Global Trigger will be redesigned and implemented in MicroTCA based technology, which allows engineers to detect all possible faults on plug-in boards, in the power supply and in the cooling system. The upgraded Global Trigger will be designed to have the same basic categories of functions as the present GT, but will have more algorithms and more possibilities for combining trigger candidates. Additionally, reconfigurability and testability will be supported based on the next system generation.},
	journal = {Journal of Instrumentation - J INSTRUM},
	author = {Rahbaran, B and Arnold, Beth and Bergauer, H and Eichberger, M and Rabady, D},
	month = dec,
	year = {2011},
}

@article{cms_collaboration_identification_2022,
	title = {Identification of hadronic tau lepton decays using a deep neural network},
	volume = {17},
	issn = {1748-0221},
	url = {http://arxiv.org/abs/2201.08458},
	doi = {10.1088/1748-0221/17/07/P07023},
	abstract = {A new algorithm is presented to discriminate reconstructed hadronic decays of tau leptons (\${\textbackslash}tau\_{\textbackslash}mathrm\{h\}\$) that originate from genuine tau leptons in the CMS detector against \${\textbackslash}tau\_{\textbackslash}mathrm\{h\}\$ candidates that originate from quark or gluon jets, electrons, or muons. The algorithm inputs information from all reconstructed particles in the vicinity of a \${\textbackslash}tau\_{\textbackslash}mathrm\{h\}\$ candidate and employs a deep neural network with convolutional layers to efficiently process the inputs. This algorithm leads to a significantly improved performance compared with the previously used one. For example, the efficiency for a genuine \${\textbackslash}tau\_{\textbackslash}mathrm\{h\}\$ to pass the discriminator against jets increases by 10-30\% for a given efficiency for quark and gluon jets. Furthermore, a more efficient \${\textbackslash}tau\_{\textbackslash}mathrm\{h\}\$ reconstruction is introduced that incorporates additional hadronic decay modes. The superior performance of the new algorithm to discriminate against jets, electrons, and muons and the improved \${\textbackslash}tau\_{\textbackslash}mathrm\{h\}\$ reconstruction method are validated with LHC proton-proton collision data at \${\textbackslash}sqrt\{s\} =\$ 13 TeV.},
	number = {07},
	urldate = {2023-04-28},
	journal = {Journal of Instrumentation},
	author = {CMS Collaboration},
	month = jul,
	year = {2022},
	note = {arXiv:2201.08458 [hep-ex, physics:physics]},
	keywords = {High Energy Physics - Experiment, Physics - Instrumentation and Detectors},
	pages = {P07023},
}

