@article{beck_galactic_1996,
	title = {Galactic {Magnetism}: {Recent} {Developments} and {Perspectives}},
	volume = {34},
	issn = {0066-4146},
	shorttitle = {Galactic {Magnetism}},
	url = {https://ui.adsabs.harvard.edu/abs/1996ARA&A..34..155B/abstract},
	doi = {10.1146/annurev.astro.34.1.155},
	abstract = {We discuss current observational and theoretical knowledge of magnetic fields, especially the large-scale structure in the disks and halos of spiral galaxies. Among other topics, we consider the enhancement of global magnetic fields in the interarm regions, magnetic spiral arms, and representations as superpositions of azimuthal modes, emphasizing a number of unresolved questions. It is argued that a turbulent hydromagnetic dynamo of some kind and an inverse cascade of magnetic energy gives the most plausible explanation for the regular galactic magnetic fields. Primordial theory is found to be unsatisfactory, and fields of cosmological origin may not even be able to provide a seed field for a dynamo. Although dynamo theory has its own problems, the general form of the dynamo equations appears quite robust. Finally, detailed models of magnetic field generation in galaxies, allowing for factors such as spiral structure, starburts, galactic winds, and fountains, are discussed and confronted with observations.},
	language = {en},
	urldate = {2022-06-30},
	journal = {Annual Review of Astronomy and Astrophysics},
	author = {Beck, Rainer and Brandenburg, Axel and Moss, David and Shukurov, Anvar and Sokoloff, Dmitry},
