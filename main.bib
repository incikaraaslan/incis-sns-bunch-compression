@phdthesis{kirchmeier_reconstruction_nodate,
	title = {Reconstruction and {Identification} of {Boosted} {Di}-{Tau} {Decays} in a {Search} for {Higgs} {Boson} {Pairs} with the {ATLAS} {Detector}},
	abstract = {{\textless}!--HTML--{\textgreater}The discovery of the Higgs boson opened the way for new experimental searches at the Large Hadron Collider at CERN.{\textless}br{\textgreater}The resonant production of two Higgs bosons is a promising signature for searches for new physics phenomena that are predicted by extensions of the Standard Model such as the Minimal Supersymmetric Standard Model. Searches for heavy resonances with masses above 1 TeV are experimentally challenging. In this regime, the Higgs bosons are produced with high momenta. Their decay products are highly collimated to the extent that the standard techniques for particle reconstruction become inefficient. {\textless}br{\textgreater}A novel method, referred to as di-\${\textbackslash}tau\$ reconstruction, is developed for the reconstruction of pairs of hadronically decaying {\textbackslash}tauleptons with a large Lorentz boost in the ATLAS detector.{\textless}br{\textgreater}Highly boosted pairs of \${\textbackslash}tau\$ leptons are reconstructed with an efficiency of more than 80\%. The multi-variate di-\${\textbackslash}tau\$ identification recognizes pairs of \${\textbackslash}tau\$ leptons with an efficiency of more than 60\%, whereas the probability to misidentify the background of a quark- or gluon-initiated jet is less than 0.01\%. The di-\${\textbackslash}tau\$ algorithms are calibrated and validated using data of 139 fb\${\textasciicircum}\{-1\}\$ of proton--proton collisions at a center-of-mass energy of 13 TeV. {\textless}br{\textgreater}These novel techniques are employed in a search for resonant production of Higgs boson pairs in the \$b{\textbackslash}bar\{b\}{\textbackslash}tau{\textasciicircum}\{+\}{\textbackslash}tau{\textasciicircum}\{-\}\$ final state.{\textless}br{\textgreater}No significant deviations from the background expectation are observed.{\textless}br{\textgreater}Upper limits are derived on the production cross section of a narrow scalar resonance times its branching ratio to decay into a Higgs boson pair.{\textless}br{\textgreater}Exclusion limits between 858 fb and 27 fb are observed at 95\% confidence level for resonance masses from 1 TeV to 3 TeV},
	language = {eng},
	author = {Kirchmeier, David},
	keywords = {Particle Physics - Experiment},
}

