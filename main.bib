@article{fromang_high_2006,
	title = {A high order {Godunov} scheme with constrained transport and adaptive mesh refinement for astrophysical magnetohydrodynamics},
	volume = {457},
	issn = {0004-6361},
	url = {https://ui.adsabs.harvard.edu/abs/2006A&A...457..371F/abstract},
	doi = {10.1051/0004-6361:20065371},
	abstract = {Aims. In this paper, we present a new method to perform numerical simulations of astrophysical MHD flows using the Adaptive Mesh Refinement framework and Constrained Transport. {\textless}BR /{\textgreater} Methods: . The algorithm is based on a previous work in which the MUSCL-Hancock scheme was used to evolve the induction equation. In this paper, we detail the extension of this scheme to the full MHD equations and discuss its properties. {\textless}BR /{\textgreater} Results: . Through a series of test problems, we illustrate the performances of this new code using two different MHD Riemann solvers (Lax-Friedrich and Roe) and the need of the Adaptive Mesh Refinement capabilities in some cases. Finally, we show its versatility by applying it to two completely different astrophysical situations well studied in the past years: the growth of the magnetorotational instability in the shearing box and the collapse of magnetized cloud cores.{\textless}BR /{\textgreater} Conclusions: . We have implemented a new Godunov scheme to solve the ideal MHD equations in the AMR code RAMSES. We have shown that it results in a powerful tool that can be applied to a great variety of astrophysical problems, ranging from galaxies formation in the early universe to high resolution studies of molecular cloud collapse in our galaxy.},
	language = {en},
	number = {2},
	urldate = {2022-07-18},
	journal = {Astronomy and Astrophysics},
	author = {Fromang, S. and Hennebelle, P. and Teyssier, R.},
	month = oct,
	year = {2006},
